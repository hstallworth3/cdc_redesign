/**********************************************/
/* Reference CSS stylesheet for NEDDS phase 2 */
/**********************************************/

/* MASTER CLASSES (for top-level HTML tags) -----------------------------------------*/
BODY
{
	margin: 0px;
	background-color: #EDEDED;
}
A
{
	color: #0000E0;
	text-decoration: underline;
}
A:hover
{
	text-decoration: underline;
}
LEGEND
{
	font-family: arial;
	font-size: 10pt;
	color: #0000A0;
	font-weight: bold;
}

TABLE
{
	border-collapse: collapse;
	margin: 0px;
	border: 0px;
}
TD
{
	padding: 0px;
	margin: 0px;
	border: 0px;
}

FIELDSET{
 border:1px solid #717D7D;
}

/* HEADER LINKS ---------------------------------------------------------------------*/
TABLE.HeaderLinks TD.Label
{
	font-family: arial;
	font-size: 9pt;
	padding-top: 1px;
	padding-bottom: 1px;
}
TABLE.HeaderLinks TD.Link
{
	font-family: arial;
	font-size: 9pt;
	font-weight: bold;
	padding-top: 1px;
	padding-bottom: 1px;
}
TABLE.HeaderLinks TD.Link A
{
	font-family: arial;
	font-size: 9pt;
	font-weight: bold;
}
TABLE.HeaderLinks TD.Separator
{
	font-family: verdana;
	font-size: 9pt;
	font-weight: bold;
	padding-left: 5px;
	padding-right: 5px;
}

/* FRAMING for DROP-DOWN MENUS ------------------------------------------------------*/
TD.MenuCorner
{
	background-color: #003470;
	width: 5px;
	height: 5px;
}
TD.MenuTopBorder
{
	background-image: url(../Images/menu_top_border.gif);
	background-repeat: repeat-x;
	background-color: #003470;
	height: 5px;
}
TD.MenuBottomBorder
{
	background-image: url(../Images/menu_bottom_border.gif);
	background-repeat: repeat-x;
	background-color: #003470;
	height: 5px;
}
TD.MenuLeftBorder
{
	background-image: url(../Images/menu_left_border.gif);
	background-repeat: repeat-y;
	background-color: #003470;
	width: 5px;
}
TD.MenuRightBorder
{
	background-image: url(../Images/menu_right_border.gif);
	background-repeat: repeat-y;
	background-color: #003470;
	width: 5px;
}
.MenuBarArea
{
	background-color: #003470;
	padding: 5px 5px 5px 10px;
}

/* DROP-DOWN MENUS ------------------------------------------------------------------*/
.GlobalNavOption
{
	font-family: arial;
	font-size: 10pt;
	font-weight: bold;
	text-decoration: none;
	color: #FFFFFF;
}
.GlobalNavOptionActive
{
	color: #F17C09;
	text-decoration: underline;
}
.GlobalNavOption:hover
{
	color: #F17C09;
}
.GlobalNavSeparator
{
	font-family: arial;
	font-size: 10pt;
	font-weight: bold;
	text-decoration: none;
	color: #D0D0D0;
	padding-left: 7px;
	padding-right: 7px;
}
DIV.SubMenu
{
	font-family: arial;
	font-size: 9pt;
	font-style: normal;
	font-weight: normal;
	color: #000000;
	background-color: transparent;
	border: none;
	padding: 4px 2px 2px 2px;
	position: absolute;
	visibility: hidden;
	z-index: 101;
}
TABLE.SubMenu
{
	background-color: #D0D0D0;
	border: 1px solid #A0A0A0;
}
.SubMenuItem
{
	font-family: arial;
	font-size: 9pt;
	font-style: normal;
	font-weight: bold;
	color: #000000;
	display: block;
	padding: 2px 4em 2px 1em;
	text-decoration: none;
	white-space: nowrap;
}
.SubMenuItem:hover
{
	color: #FFFFFF;
	background-color: #3366CC;
	text-decoration: none;
}
.SubMenuItemActive
{
	text-decoration: underline;
}

