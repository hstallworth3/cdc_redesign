<!DOCTYPE form-validation PUBLIC "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.3.0//EN"
     "http://jakarta.apache.org/commons/dtds/validator_1_3_0.dtd">
<form-validation>
	<!--
     This is a minimal Validator form file with a couple of examples.
-->
	<global>
		<constant>
			<constant-name>phone</constant-name>
			<constant-value>^\(?(\d{3})\)?[-| ]?(\d{3})[-| ]?(\d{4})$</constant-value>
		</constant>
		<constant>
			<constant-name>zip</constant-name>
			<constant-value>^\d{5}(-\d{4})?$</constant-value>
		</constant>
		<constant>
			<constant-name>budgetAmt</constant-name>
			<constant-value>^(?!0,?\d)\d{1,8}(\.\d{2})?$</constant-value>
		</constant>
	</global>
	<formset>
		<!-- An example form -->
		<form name="TBForm">
			<field property="personVO.personNameDT_s[0].lastNm" depends="required">
				<arg key="rvct.last.name"/>
			</field>
			<field property="personVO.personNameDT_s[0].firstNm" depends="required">
				<arg key="rvct.first.name"/>
			</field>
			<field property="investigationProxyVO.publicHealthCaseVO_s.thePublicHealthCaseDT.jurisdictionCd" depends="required">
				<arg key="rvct.tab1.jurisdiction"/>
			</field>
			<field property="investigationProxyVO.publicHealthCaseVO_s.thePublicHealthCaseDT.progAreaCd" depends="required">
				<arg key="rvct.tab1.program.area"/>
			</field>
			<field property="obs.TB100.obsValueTxtDT_s[0].valueTxt" depends="required">
				<arg key="rvct.tab1.state.case.number"/>
			</field>
			<field property="fb.fields.DateReported.string" depends="required">
				<arg key="rvct.tab1.date.reported"/>
			</field>
			<field property="postalDT.zipCd" depends="mask">
				<arg key="rvct.zip"/>
				<var>
					<var-name>mask</var-name>
					<var-value>${zip}</var-value>
				</var>
			</field>
		</form>
		 
		<form name="epiLinkIdForm">
			<field property="currentEpiLinkId" depends="required">
				<arg key="epi.link.id"/>
			</field>
			<field property="newEpiLinkId" depends="required">
				<arg key="new.epi.link"/>
			</field>
			
		</form>

		
	</formset>
</form-validation>
