		<!--/nedss:If-->
		<button-bar>
			<!--nedss:If task="CreateInvestigation"-->
			<right tie-to="Create|Edit">
				<label>Submit</label>
				<javascript-action>submitForm();</javascript-action>
			</right>
			<right tie-to="Create|Edit">
				<label>Cancel</label>
				<javascript-action>cancelForm('<xsp-request:get-attribute name="Cancel"/>');</javascript-action>
			</right>
