			<!-- ====================== MAPPING FOR PRINTABLE FORMS ============================== -->
			<map:match pattern="investigation_pertussis_print">
				<map:aggregate element="page">
					<map:part src="cocoon://person/patient_from_event_print_view"/>
					<map:part src="cocoon:/investigation_pertussis_print_int"/>
					<map:part src="cocoon://diseaseform/contacttracing/ContactTracingPrint"/>
