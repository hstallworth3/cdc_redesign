			</map:match-->
			<map:match pattern="pertussis_input_int">
				<map:aggregate element="page">
					<map:part src="cocoon:/pertussis_input_src"/>
				</map:aggregate>
				<map:transform src="cocoon://sub-aggregate-xsl-source"/>
				<map:serialize type="xml"/>
			</map:match>
