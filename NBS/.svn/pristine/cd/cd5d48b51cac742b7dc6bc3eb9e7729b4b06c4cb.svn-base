<%@ include file="/jsp/tags.jsp" %><%@ page isELIgnored ="false" %><html>    <head>        <title>${BaseForm.pageTitle}</title>        <%@ include file="/jsp/resources.jsp" %>        <SCRIPT LANGUAGE="JavaScript" Src="localFields.js"></SCRIPT>        <SCRIPT LANGUAGE="JavaScript" src='/nbs/dwr/interface/JLocalFieldForm.js'></SCRIPT>        <SCRIPT LANGUAGE="JavaScript">	        function add()	        {				document.forms[0].action ="/nbs/LocalFields.do?method=createLoadLDF#localField";			}		   		function previewLFs()	   		{				window.location="/nbs/LocalFields.do?method=previewLDF";				document.forms[0].action ="/nbs/LocalFields.do?method=previewLDF";				return true;			}						function checkLDFDelete()			{				var checkErrorConditions = getElementByIdOrByName("deleteError");				if(checkErrorConditions!=null){					if(checkErrorConditions.value!="")						alert(checkErrorConditions.value);				}			}								function checkDuplicateDMColumn()			{				var checkDuplicates = getElementByIdOrByName("duplicateDMColumn");				if(checkDuplicates !=null){					if(checkDuplicates.value!="")						getElementByIdOrByName("r-datamartColumnNm").style.color="red";				}			}				        </SCRIPT>    </head>    <body onLoad="ldfsOnLoad();checkLDFDelete();checkDuplicateDMColumn();">        <div id="doc3">            <div id="bd">                <!-- Top nav -->	             <%@ include file="../../jsp/topNavFullScreenWidth.jsp" %>                <div style="text-align:right; width:100%;"  id="ldfLink">                     <a id="manageLink" href="LDFAdminLoad.do">Return to Custom Field Administration</a>                    <input type="hidden" id="actionMode" value="${localFieldsForm.actionMode}"/>                </div>                                <!-- Top button bar -->                <table style="background-image: url('task_button/tb_cel_bak.jpg');background-repeat: repeat-x;" class="topButtonBar">                    <tr>				        <!-- Investigation related actions -->				        <td style="vertical-align:top; padding:0px;">			                <table align="right">			                    <tr>                                    <td style="vertical-align:top; padding-top:0px;">		                                <input id="preview1" type="image" src="task_button/fa_submit.jpg"  width="30" height="40"                                        border="0" name="Submit" id="Submit" alt="Preview LDFs" class="cursorHand" onclick="return previewLFs();"><br/>                                        Preview                                    </td>                                </tr>                            </table>                        </td>                    </tr>                </table>                					<div style="text-align:right; width:100%;"> 					<span class="boldTenRed"> * </span>					<span class="boldTenBlack"> Indicates a Required Field </span>  				</div>                       				                <!-- Page starts below -->				                <table cellpadding="0" cellspacing="0" border="0" style="width:100%;">					<tr>						<td>							<table>								<tr>								    <td style="WIDTH: 15px;"></td>                                    <td class="none" id="error"></td>								</tr>                               							</table>						</td>					</tr>					<tr>						<td>				                            <html:form action="/LocalFields.do">  						        <!-- Manage Local Fields -->						        <nedss:container id="section1" name="Manage Local Fields" classType="sect" displayImg ="false" displayLink="false">	                                <fieldset  style="border-width:0px;"  id="result">	                                    <table width="98%">	                                        <tr>									            <td align="center">									        	   <display:table name="manageList" class="dtTable" pagesize="10"  id="parent" requestURI="">										                <display:column property="viewLink" title="" class="dstag"/>										                <display:column property="editLink" title="" class="dstag"/>										                <display:column property="deleteLink" title="" class="dstag"/>										                <display:column property="questionLabel" title="Label" class="dstag"/>										                <display:column property="typeCdDesc" title="Type" class="dstag"/>										                <display:column property="tab" title="Tab" class="dstag"/>										                <display:column property="section" title="Section" class="dstag"/>		                                                <display:column property="subSection" title="Subsection" class="dstag"/>			            										                <display:column property="orderNbr" title="Display Order" class="dstag"/>										                <display:setProperty name="basic.empty.showtable" value="true"/>									                </display:table>	                                            </td>	                                        </tr> 	                                        <tr>										      	<td class="InputButton" align="right">										      		<input type="submit" name="submit" id="submit" value="Add New LDF" onClick="add();"/>										      	</td>	                                        </tr>	                                    </table>	                                </fieldset>	                            </nedss:container>									 <logic:equal name="localFieldsForm" property="ldfPageId" value="300">		                            <logic:notEqual name="localFieldsForm" property="actionMode" value="Manage">			                            <%@ include file="HomePage_LocalField.jsp" %>				                            </logic:notEqual>									 </logic:equal>	                            <logic:notEqual name="localFieldsForm" property="ldfPageId" value="300">		                            <logic:notEqual name="localFieldsForm" property="actionMode" value="Manage">		                                <%@ include file="LocalField.jsp" %>				                            </logic:notEqual>	                            </logic:notEqual>								 	                            	                            <input type="hidden" name="deleteError" value="<%= request.getAttribute("deleteError") == null ? "" : request.getAttribute("deleteError")%>"/>	                            <input type="hidden" name="duplicateDMColumn" value="<%= request.getAttribute("duplicateDMColumn") == null ? "" : request.getAttribute("duplicateDMColumn")%>"/>                            </html:form>                        </td>                    </tr>                </table>                				<!-- Bottom button bar -->	            <table style="background-image: url('task_button/tb_cel_bak.jpg');background-repeat: repeat-x;" class="bottomButtonBar">	                <tr>	                    <td style="vertical-align:top; padding:0px;">	                        <table align="right">	                            <tr>	                                <td style="vertical-align:top; padding-top:0px;">                                        <input id="preview2" type="image" src="task_button/fa_submit.jpg"  width="30" height="40"                                        border="0" name="Submit" id="Submit" alt="Preview LDFs" class="cursorHand" onclick="return previewLFs();"><br/>                                        Preview	                                </td>	                            </tr>	                        </table>	                    </td>	                </tr>	            </table>	                        </div>        </div>    </body></html>	