<%@ page import="java.util.Collection, java.util.ArrayList" %><%@ page import="gov.cdc.nedss.report.dt.ReportDT,                 gov.cdc.nedss.util.NEDSSConstants,                 gov.cdc.nedss.util.XMLRequestHelper,                 gov.cdc.nedss.report.util.ReportConstantUtil,                de.nava.informa.core.ParseException" %><nedss:dashlet id="feeds" name="News Feeds"         isDraggable="true" headerClass="header dd-multi-handle-${colIndex}"         headerId="dd-handle-${colIndex}_${rowIndex}a" contentStyle="height=${dashletHeight}px;">    <logic:notEmpty name="homepageForm" property="feedsCollection">        <logic:iterate id="link" name="homepageForm" property="feedsCollection">            <nedss:simpleRSS uri="${link}" />            <br/>        </logic:iterate>    </logic:notEmpty>        <logic:empty name="homepageForm" property="feedsCollection">        <span style="color:green;">            You are not setup to recieve any news feeds. Please contact your system            administrator to have them configure it for you.        </span>    </logic:empty></nedss:dashlet>