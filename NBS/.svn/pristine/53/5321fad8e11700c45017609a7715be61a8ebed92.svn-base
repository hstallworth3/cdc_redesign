div.suggestionList {
	z-index:500;
	position:absolute;
	width:150px; 
	border:1px solid #333333; 
	background-color:#ffffff; 
	font-family:Tahoma; 
	font-size:11px; 
	cursor:default; 
	visibility:hidden;
}
div.suggestionList_element_off {
	width: 148px;
	background: #ffffff;
	color: #333333;
	padding-left: 2px;
}
div.suggestionList_element_on {
	width: 148px;
	background: #6D99C6;
	color: #ffffff;
	padding-left: 2px;
}
div#parentWindowDiv, 
#pamview {
	display:none;
	position:absolute;
	left:0px;
	top:0px;
	width:100%;
	height:1000%;
	background:gray;
	filter:alpha(Opacity=80);
	opacity:0.5;
	-moz-opacity:0.5;
	-khtml-opacity:0.5;
	z-index:2;
}
table.viewpam {
	margin: 5px 0 1px 0 !important;
}