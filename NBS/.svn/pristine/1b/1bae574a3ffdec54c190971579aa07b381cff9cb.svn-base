<%@ include file="/jsp/tags.jsp" %><html>    <head>    <title><%= request.getAttribute("PageTitle") %></title>        <%@ include file="/jsp/resources.jsp" %>        <script type="text/javascript" src="srtadmin.js"></script>		          <script type='text/javascript' src='/nbs/dwr/interface/JSRTForm.js'></script>				        <script language="JavaScript">            function findComponent()            {                var index = 0;                var errors = new Array();                var isError = false;					                var criteria  = getElementByIdOrByName("labTest");                                if(criteria != null && criteria.value.length == 0)                {                    isError = true;                    errors[index++] = "Lab Test Description is required"                    getElementByIdOrByName("tst").style.color="red";                }                else {                    getElementByIdOrByName("tst").style.color="black";                }                                if(isError) {                    $j("#zeroResultsMsg").hide();                    displayGlobalErrorMessage(errors);                    return false;                   }                document.forms[0].action ="/nbs/LabTestLoincLink.do?method=labTestSearchSubmit";				            }            function populateLabTest(cd, labId)            {                 var parent = window.opener;                var parentDoc = parent.document;				                dwr.util.setValue(getElementByIdOrByNameNode("searchCriteria(LABID)",parentDoc), labId);                dwr.util.setValue(getElementByIdOrByNameNode("searchCriteria(LABTEST)",parentDoc), cd);	                dwr.util.setValue(getElementByIdOrByNameNode("searchCriteria(LOINC)",parentDoc), '');						   						                dwr.util.setValue(getElementByIdOrByNameNode("labId",parentDoc), labId);                dwr.util.setValue(getElementByIdOrByNameNode("labTest",parentDoc), cd);				                self.close();			}                        function closePopup()            {                self.close();                var block= getElementByIdOrByNameNode("blockparent", opener.document)                block.style.display = "none";					            }                        function unhideParent()            {                var block= getElementByIdOrByNameNode("blockparent", opener.document)                block.style.display = "none";					            }			            function bonload()            {                autocompTxtValuesForJSP();                getElementByIdOrByName('labTest').focus();											            }		</script>	</head>    <body onload="bonload();">        <div id="doc3">            <div id="bd">                <%@ include file="/jsp/popupUtil.jsp" %>                <%@ include file="../../../jsp/feedbackMessagesBar.jsp" %>                                <!-- 0 search results message -->                <logic:notEmpty name="SRTAdminManageForm" property="attributeMap.NORESULT">                    <table style="width:100%;">                        <tr>                            <td>                                <div class="infoBox messages" id="zeroResultsMsg">                                    Your Search Criteria resulted in 0 possible matches.                                    Please refine your search and try again.                                </div>                            </td>                        </tr>                    </table>                </logic:notEmpty>                                <html:form action="/LabTestLoincLink.do">                    <nedss:container id="section1" name=" Find an existing LabTest"                             classType="sect" displayImg ="false" displayLink="false">                        <fieldset style="border-width:0px;" >	                        <nedss:container id="subsec1" classType="subSect" displayImg ="false">	                            <tr>	                                <td class="fieldName" id="tst"><font class="boldTenRed" > * </font><span>Lab Test Description:</span></td>	                                <td>	                                    <html:select styleId="srchCriteria" property="searchCriteria(SRCH_CRITERIA)" acomplete="false">	                                        <html:optionsCollection property="searchCriteriaDropDown" value="key" label="value"/>	                                    </html:select>						 	                                    <html:text styleId="labTest" property="searchCriteria(LABTEST)" size="25" maxlength="100"/>	                                </td>	                            </tr>	                            <tr>	                                <td class="fieldName" ><span>Lab Name:</span></td>	                                <td>	                                    <html:select property="searchCriteria(LABID)" styleId="srchLabId">	                                        <html:optionsCollection property="laboratoryIds" value="key" label="value"/>	                                    </html:select>					 	                                </td>	                            </tr>			  		                            <tr>	                                <td colspan="2" align="right">	                                    <table>	                                        <tr>	                                            <td class="InputField">	                                                <input type="submit" name="submit" id="submit" value="Search" onClick="return findComponent();"/>	                                                <input type="submit" name="submit" id="submit" value="Cancel" onClick="closePopup();"/>			  	                                                &nbsp;	                                            </td>	  		                                        </tr>  	                                    </table>	                                </td>	                            </tr>	                        </nedss:container>                        </fieldset>                    </nedss:container>                                    <!-- search results -->	                <logic:notEmpty name="SRTAdminManageForm" property="manageList">  	                    <nedss:container id="section2" name=" Search Results " classType="sect"  displayImg ="false" displayLink="false">	                        <fieldset style="border-width:0px;" >	                            <table width="98%">	                                <tr>	                                    <td align="center">	                                        <display:table name="manageList" class="dtTable" pagesize="10"  id="parent" requestURI="">	                                        <display:column property="viewLink" title="Lab Test Code" />	                                        <display:column property="labTestDescTxt" title="Lab Test Description" />	                                        <display:column property="laboratoryId" title="Lab Id"/>	                                        <display:setProperty name="basic.empty.showtable" value="true"/>	                                        </display:table>	                                    </td>	                                </tr>       		                            </table>	                        </fieldset>	                    </nedss:container>	                </logic:notEmpty>                </html:form>            </div>        </div>    </body></html>