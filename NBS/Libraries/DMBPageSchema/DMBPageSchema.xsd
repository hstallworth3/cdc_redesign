<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:util="http://www.cdc.gov/nedss/pagemanagement/wa/xml//util" targetNamespace="http://www.cdc.gov/nedss/pagemanagement/wa/xml//util" elementFormDefault="unqualified" attributeFormDefault="unqualified">
	<xs:element name="PageInfo" type="util:PageInfoType"/>
	<xs:complexType name="PageInfoType">
		<xs:annotation>
			<xs:documentation>PageInfo is the root element. The schema is intended to hold the metadata for a single Page containing one or more Tabs.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Header" type="util:HeaderType"/>
			<xs:element name="Page" type="util:PageType"/>
			<xs:element name="Template" type="util:PageTemplateType"/>
			<xs:element name="QuestionLibEntry" type="util:QuestionLibraryType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="UIMetaEntry" type="util:UserInterfaceMetadataType" minOccurs="0" maxOccurs="unbounded"/>
