			<xs:element name="PageRule" type="util:PageRuleType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="HeaderType">
		<xs:annotation>
			<xs:documentation>Specifies the Version of the Schema and high level information about the Page.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Status" type="xs:string" default="Published"/>
			<xs:element name="VersionID" type="xs:string" default="May 18, 2010" minOccurs="0"/>
			<xs:element name="PageTemplateUsed" type="xs:string" minOccurs="0"/>
