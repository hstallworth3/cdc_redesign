<%@ include file="/jsp/errors.jsp" %><title>Manage Link between LOINC and Condition</title><script type="text/javascript">    function cancelForm()    {        var confirmMsg="If you continue with the Cancel action, you will lose any information you have entered. Select OK to continue, or Cancel to not continue.";        if (confirm(confirmMsg)) {            document.forms[0].action ="${SRTAdminManageForm.attributeMap.cancel}";        } else {            return false;        }    }	    function submitForm()    {        if(LoincCondLinkCreateReqFlds()) {            return false;        } else {            document.forms[0].action ="${SRTAdminManageForm.attributeMap.submit}";        }    }</script><html:form styleId="createForm" action="/LoinctoConditionLink.do">    <nedss:container id="section3" name="${SRTAdminManageForm.actionMode} LOINC Condition Link "  classType="sect" displayImg ="false" displayLink="false">        <fieldset style="border-width:0px;" id="Add">            <nedss:container id="subsec2" classType="subSect" displayImg ="false">                <!-- Form Entry Errors -->                <tr style="background:#FFF;">                    <td colspan="2">                        <div class="infoBox errors" style="display:none;" id="srtDataFormEntryErrors">                        </div>                                            </td>                </tr>                                <!-- Data form -->                <tr>                    <td class="fieldName" id="loin"><font class="boldTenRed" > * </font><span>LOINC Code:</span></td>                    <td>                        <logic:equal  name="SRTAdminManageForm" property="actionMode" value="Create">                            <span id="loinc1">${SRTAdminManageForm.searchMap.LOINC}</span>                            <logic:empty name="SRTAdminManageForm" property="searchMap.LOINC">                                <input type="button" name="submit" id="submit" value="Search LOINC" onClick="searchLoinc();"/>                            </logic:empty>                        </logic:equal>                                                <logic:equal  name="SRTAdminManageForm" property="actionMode" value="View">                                          <nedss:view name="SRTAdminManageForm" property="selection.loincCd"/>                        </logic:equal>                    </td>                </tr>                            <tr>                    <td class="fieldName" id="cond"><font class="boldTenRed" > * </font><span>Condition:</span></td>                    <td>                        <logic:equal  name="SRTAdminManageForm" property="actionMode" value="Create">                                        <html:select property="searchCriteria(CONDITION)" styleId="condition">                                <html:optionsCollection property="conditionList" value="key" label="value"/>                            </html:select>                            </logic:equal>                                                            <logic:equal  name="SRTAdminManageForm" property="actionMode" value="View">                                          <nedss:view name="SRTAdminManageForm" property="selection.conditionCd"  methodNm="AllConditions"/>                        </logic:equal>                    </td>                </tr>                                <logic:equal  name="SRTAdminManageForm" property="actionMode" value="Create">                    <tr>                        <td colspan="2" align="right" class="InputField">                            <input type="submit" name="submit" id="submit" value="Submit" onClick="return submitForm();"/>                            <input type="submit" name="submit" id="submit" value="Cancel" onClick="return cancelForm();"/>			                              &nbsp;                        </td>                    </tr>         			  	                </logic:equal>		            </nedss:container>        </fieldset>	     </nedss:container></html:form>