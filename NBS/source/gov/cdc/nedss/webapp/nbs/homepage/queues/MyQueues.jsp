<nedss:dashlet id="myQueues" name="My Queues" 	    isDraggable="true" headerClass="header dd-multi-handle-${colIndex}" 	    contentStyle="height=${dashletHeight}px;"	    headerId="dd-handle-${colIndex}_${rowIndex}a">    <logic:notEmpty name="homepageForm" property="queueCollection">        <ul style="margin-left:0.75em;">            <logic:iterate id="link" name="homepageForm" property="queueCollection">                <li style="list-style:circle; margin-bottom:6px;">                    <bean:write name="link" filter="false" />                 </li>            </logic:iterate>        </ul>    </logic:notEmpty>        <logic:empty name="homepageForm" property="queueCollection">        There are no shortcuts available.    </logic:empty> </nedss:dashlet>