<%@ page import="java.util.Collection,                gov.cdc.nedss.report.dt.ReportDT,                 gov.cdc.nedss.util.NEDSSConstants,                 gov.cdc.nedss.util.XMLRequestHelper,                 gov.cdc.nedss.report.util.ReportConstantUtil,                gov.cdc.nedss.webapp.nbs.form.homepage.HomePageForm" %><nedss:dashlet id="myReportsDashlet" name="My Reports"         isDraggable="true" headerClass="header dd-multi-handle-${colIndex}"         headerId="dd-handle-${colIndex}_${rowIndex}a" contentStyle="height=${dashletHeight}px;">    <logic:notEmpty name="homepageForm" property="reportCollection">        <display:table name="${homepageForm.reportCollection}" class="HomePageDT inPageSortable" id="myReports">            <display:column title="" style="width:4em; text-align:center;">                <% if (((ReportDT)pageContext.getAttribute("myReports")).isRunPermission()) { %>                    <a href="javascript:runReport('<%=((ReportDT)pageContext.getAttribute("myReports")).getShared()%>',<%=((ReportDT)pageContext.getAttribute("myReports")).getReportUid()%>,<%=((ReportDT)pageContext.getAttribute("myReports")).getDataSourceUid()%>)"> Run </a>                <% } else {%>                    <span style="color:#BBB;">Run</span>                <%}%>            </display:column>            <display:column title="Report Title" class="hoverDescLink">                <a href="javascript:void(0)" title="<%=((ReportDT)pageContext.getAttribute("myReports")).getDescTxt()%>"> <%=((ReportDT)pageContext.getAttribute("myReports")).getReportTitle()%> </a>            </display:column>            <display:column property="addTime" title="Date Created" format="{0,date,MM/dd/yyyy}" style="width:7em;"/>        </display:table>    </logic:notEmpty>         <logic:empty name="homepageForm" property="reportCollection">        There are no private reports available.    </logic:empty></nedss:dashlet>            <script type="text/javascript">    function runReport(reportType, reportUID, reportDataSourceId)        {            // FIXME : Since the current logic to run all report types is the same,            // there is a single if grouping.            if (reportType == "P" || reportType == "S" || reportType == "T" || reportType == "R")            {                // set the form values and post the form                 document.RunReportForm.mode.value = "edit";                document.RunReportForm.ObjectType.value = <%= NEDSSConstants.REPORT %>;                document.RunReportForm.OperationType.value = <%= NEDSSConstants.REPORT_BASIC %>;                document.RunReportForm.DataSourceUID.value = reportDataSourceId;                document.RunReportForm.ReportUID.value = reportUID;                document.RunReportForm.action = "/nbs/nfc";                document.RunReportForm.method = "post";                document.RunReportForm.target = "_self";                document.RunReportForm.submit();            }        }                /* JQuery stuff. In page sorting of table data is handled here. */        $j(document).ready(function() {             // extend the default setting to always include the zebra widget.             $j.tablesorter.defaults.widgets = ['zebra'];            // call the tablesorter plugin            $j("#myReports").tablesorter(                                        {                                            headers : {                                                        0:{                                                            sorter:false                                                           }                                                      },                                            sortList: [[2,1]]                                                    }                                      );            });                                          </script>